<launch>

    <node pkg='turtlebot4_controller' exec='turtlebot4_controller_node' />

    <node pkg='ros_ign_bridge' exec='parameter_bridge'
        args='/model/object1/pose@geometry_msgs/msg/Pose[ignition.msgs.Pose /model/object2/pose@geometry_msgs/msg/Pose[ignition.msgs.Pose /model/object3/pose@geometry_msgs/msg/Pose[ignition.msgs.Pose /model/object4/pose@geometry_msgs/msg/Pose[ignition.msgs.Pose'>
    </node>

    <include file='$(find-pkg-share turtlebot4_ignition_bringup)/launch/turtlebot4_ignition.launch.py'>
        <arg name='world' value='arena'/>
        <arg name='z' value='0.2'/>
    </include>


</launch>
